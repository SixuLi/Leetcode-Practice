# Definition for singly-linked list.
class ListNode:
    def __init__(self, x):
        self.val = x
        self.next = None

class Solution:
    def mergeTwoLists(self, l1: ListNode, l2: ListNode) -> ListNode:
        cur = ListNode(0)
        res = cur
        p1, p2 = l1, l2
        while p1 and p2:
            if p1.val > p2.val:
                node = ListNode(p2.val)
                p2 = p2.next
            else:
                node = ListNode(p1.val)
                p1 = p1.next
            cur.next = node
            cur = cur.next
        p = p1 if p1 else p2
        cur.next = p
        return res.next