# Definition for singly-linked list.
class ListNode:
    def __init__(self, x):
        self.val = x
        self.next = None


class Solution:
    def removeNthFromEnd(self, head: ListNode, n: int) -> ListNode:
        dummy = ListNode(0)
        dummy.next = head
        res = dummy
        p1, p2 = dummy, dummy
        while n != 0:
            p2 = p2.next
            n -= 1
        while p2.next:
            p1 = p1.next
            p2 = p2.next
        if p1.next == p2:
            p1.next = None
        else:
            p1.next = p1.next.next
        return res.next
