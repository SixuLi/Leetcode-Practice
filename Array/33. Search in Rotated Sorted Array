class Solution:
    def search(self, nums: List[int], target: int) -> int:
        numsLen = len(nums)
        if numsLen == 0:
            return -1
        l, r = 0, numsLen-1
        while l <= r:
            mid = (l+r) // 2
            if nums[mid] == target:
                return mid
            if nums[mid] < nums[r]:
                if target > nums[mid] and target <= nums[r]:
                    l = mid+1
                else:
                    r = mid-1
            else:
                if target < nums[mid] and target >= nums[l]:
                    r = mid-1
                else:
                    l = mid+1
        return -1